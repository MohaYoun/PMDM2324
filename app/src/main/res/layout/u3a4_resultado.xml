<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ut03.u3a4DevResultado">

    <EditText
        android:id="@+id/u3a4etNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/u3a4btAceptar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="Aceptar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/u3a4etNombre" />

    <Button
        android:id="@+id/u3a4btLimpiar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Limpiar"
        app:layout_constraintBottom_toBottomOf="@+id/u3a4btAceptar"
        app:layout_constraintStart_toEndOf="@+id/u3a4btAceptar"
        app:layout_constraintTop_toTopOf="@+id/u3a4btAceptar" />

    <Button
        android:id="@+id/u3a4btCancelar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="@+id/u3a4btLimpiar"
        app:layout_constraintStart_toEndOf="@+id/u3a4btLimpiar"
        app:layout_constraintTop_toTopOf="@+id/u3a4btLimpiar" />
</androidx.constraintlayout.widget.ConstraintLayout>